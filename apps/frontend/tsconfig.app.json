{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "src/types/env.d.ts",
    "src/types/svg.d.ts",
    "src",
    "src/**/*",
    "src/**/*.vue"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@zod/db/*": [
        "../../packages/shared/zod/db/*"
      ],
      "@zod/dto/*": [
        "../../packages/shared/zod/dto/*"
      ],
      "@zod/generated": [
        "../../packages/shared/zod/db/generated"
      ],
      "@shared/*": [
        "../../packages/shared/*"
      ],
    }
  }
}