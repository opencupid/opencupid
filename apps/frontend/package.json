{
  "name": "frontend",
  "version": "0.6.2",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "dev:depcruise": "depcruise   --output-type json src > docs/deps.json && depcruise   --output-type dot src | dot -T svg > docs/deps.svg",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "vue-i18n-extract": "vue-i18n-extract report --vueFiles './src/**/*.?(ts|vue)' --languageFiles '../../packages/shared/i18n/*.?(json|yml|yaml|js)'"
  },
