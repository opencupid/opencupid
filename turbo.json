{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ],
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {},
    "lint": {},
    "generate": {
      "outputs": []
    },
    "migrate": {
      "outputs": []
    },
    "format": {
      "outputs": []
    }
  }
}
